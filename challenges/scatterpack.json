{
    "name": "Scatterpack",
    "description": {
        "short": "Determine if arrays contain the same elements.",
        "full": "Determine if two shuffled arrays of numbers contain the same elements. You cannot use `Array.prototype.sort()`."
    },

    "added_by": "anyweez",
    "added_on": "2016-08-20",
    "status": {
        "available": true
    },

    "spec": {
        "func": "scatterpack",
        "parameters": ["array", "array"],
        "returns": "boolean",
        "examples": [{
            "inputs": ["[1, 2, 3]", "[3, 2, 1]"],
            "output": true
        },{
            "inputs": ["[1, 1, 2]", "[1, 2, 1]"],
            "output": true
        },{
            "inputs": ["[4, 9, 1, 8]", "[4, 9]"],
            "output": false
        }]
    },
    
    "tests": "scatterpack.js"
}