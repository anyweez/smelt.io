{
    "name": "Junior",
    "description": {
        "short": "Find the second largest number in an array.",
        "full": "Find the second largest number in the provided array. If the largest value exists twice, it counts as both the largest and second largest value."
    },

    "added_by": "anyweez",
    "added_on": "2016-08-17",
    "status": {
        "available": true
    },

    "spec": {
        "func": "junior",
        "parameters": ["array"],
        "returns": "number",
        "examples": [{
            "inputs": [[1, 5, 4, 1]],
            "output": "4"
        },{
            "inputs": [[10, 14, 17, 9, 18]],
            "output": "17"
        },{
            "inputs": [[1, 2, 3, 5, 5]],
            "output": "5"
        }]
    },
    
    "tests": "junior.js"
}